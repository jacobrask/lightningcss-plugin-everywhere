{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["esnext"],
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strict": true
  }
}
